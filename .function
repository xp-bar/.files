    fi
}

function routes() {
    local i=0
    local fzf_search=""

    while [[ $i -lt $# ]]; do
        i=$[$i+1]
        local argument=$argv[$i]

        if [[ $argument != *"--"* ]]; then
            fzf_search=$argument
            i=$#
        fi
    done

    local cache_dir="$HOME/.cache"
    local project_name=${PWD##*/}
    local file_name="$cache_dir/artisan_routes/.$project_name"

    if [[ ! -d $cache_dir ]]; then
        echo "No $HOME/.cache directory."
        return 1
    fi

    [[ ! -d $cache_dir/artisan_routes ]] && mkdir -p $cache_dir/artisan_routes
    [[ ! -f $file_name ]] && touch $file_name

    if [[ $* = *"--rebuild"* ]]; then
        echo "" > $file_name
        echo "$(php artisan route:list)" > $file_name
    fi

    local search="\$6"

    if [[ $* = *"--url"* ]]; then
        search="\$4"
    fi

    if [[ $* = *"--controller"* ]]; then
        search="\$8"
    fi

    local searchable="$(\
        cat $file_name |\
        tail -n +4 |\
        sed 's/| / /' |\
        awk '{print '$search', "+++", $6}' |\
        grep -v '|' |\
        awk '!seen[$0]++' |\
        grep -v '^ +++ '
    )"
    
    local route_name=""

    if [[ $fzf_search = "" ]]; then
        result="$(echo "$searchable" | sed 's:+++.*::g' | fzf)"
    else
        result="$(echo "$searchable" | sed 's:+++.*::g' | fzf -q "$fzf_search")"
    fi

    result="$(echo "$result" | sed 's:\\:.:g')"
    route_name="$(echo "$searchable" | grep "$result" | sed 's:.*+++::g' | sed 's:^[ ]*::g')"


    if [[ $route_name = "" ]]; then
        return 1
    fi

    if [[ $* = *"--show-url"* ]]; then
        php artisan route:list --name="$route_name" |\
            tail -n +4 |\
            sed 's/| / /' |\
            awk '{print $4}' |\
            grep -v '^[ ]*$'
    elif [[ $* = *"--show-controller"* ]]; then
        php artisan route:list --name="$route_name" |\
            tail -n +4 |\
            sed 's/| / /' |\
            awk '{print $8}' |\
            grep -v '^[ ]*$'
    elif [[ $* = *"--edit"* ]]; then
        local namespace=${ROOT_NAMESPACE:-""}
        local filename="$(php artisan route:list --name="$route_name" |\
            tail -n +4 |\
            sed 's/| / /' |\
            awk '{print $8}' |\
            grep -v '^[ ]*$' |\
            sed 's/@.*$//g' |\
            sed 's:\\:/:g' |\
            sed "s:"$namespace":app:g")"

        $EDITOR $filename".php"
    else
        php artisan route:list --name="$route_name"
    fi
}

