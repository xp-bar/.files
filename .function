function loader(){
    COLS=$(tput cols);
    AMOUNT=${1:-$COLS};
    AMOUNT=$[$AMOUNT/100*$COLS]
    c;
    hide;
    POS=$[$COLS%2]
    for i in `seq 0 $[$AMOUNT%$COLS]`;
        do
            tput cup $POS $i;
            echo -e '\u2503';
            sleep 0.05;
        done;
    read "?Done!";
    show
}

