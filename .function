gpr() {
    local arg1=$1
    local base=${arg1:-"master"}
    local assigned=""
    local label=""

    if [[ $@ == *"-a"* ]]; then
        assigned="$(_collabs | fzf --header='Select an assignee:')"
    fi

    if [[ $@ == *"-l"* ]]; then
        label="$(_labels | fzf --header='Select a label:')"
    fi

    if [[ $assigned != "" && $label != "" ]]; then
        hub pull-request -a $assigned -r $assigned -l $label -b $base
    elif [[ $assigned != "" && $label = "" ]]; then
        hub pull-request -a $assigned -r $assigned -b $base
    elif [[ $assigned = "" && $label != "" ]]; then
        hub pull-request -l $label -b $base
    else
        hub pull-request -b $base
    fi
}


